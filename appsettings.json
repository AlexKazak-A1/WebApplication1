{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ForwardedHeaders": {
    "ForwardedHeaders": "All"
  },
  "AllowedHosts": "*",
  "HTTP_PROXY": "${HTTP_PROXY}", 
  "HTTPS_PROXY": "${HTTPS_PROXY}", 
  "NO_PROXY": "${NO_PROXY}",
  "OverSubscriptionLimit": 2.5, 
  "CPUUsageLimit": 0.8, 
  "CPUMin": 2,
  "RAMMin": 4,
  "Login": "${LOGIN}",
  "Password": "${PASSWOWD}",
  "DEX_URL": "${DEX_URL}",
  "DEX_ClientId": "${DEX_CLIENT_ID}",
  "DEX_ClientSecret": "${DEX_CLIENT_SECRET}"
}
