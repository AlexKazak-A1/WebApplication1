@{
    ViewData["Title"] = "Home Page";
}
@model ProvisionModel

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <form id="ProvisionForm">
        <div>
            <button type="submit" id="addProxmox" name="addProxmox" asp-controller="Proxmox" asp-action="AddNewProxmox" style="margin-bottom: 10px">Add Proxmox</button>
        </div>
        <div>
            <button type="submit" id="addRancher" name="addRancher" asp-controller="Rancher" asp-action="AddNewRancher" style="margin-bottom: 10px">Add Rancher</button>
        </div>

        <div>
            <button type="submit" id="toProvision" name="toProvision" asp-controller="Provision" asp-action="Index" style="margin-bottom: 10px">Provision new Cluster</button>
        </div>
    </form>
</div>
